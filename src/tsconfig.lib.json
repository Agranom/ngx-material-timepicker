{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "outDir": "../out-tsc/app",
        "baseUrl": "./",
        "types": []
    },
    "files": [
        "main.ts",
        "polyfills.ts"
    ],
    "include": [
        "src/**/*.d.ts"
    ],
    "exclude": [
        "src/test.ts",
        "**/*.spec.ts"
    ],
    "angularCompilerOptions": {
        "enableIvy": true,
        "compilationMode": "partial"
    }
}
